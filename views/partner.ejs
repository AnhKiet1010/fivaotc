    <%- include ('./partials/header') %>
    <div id="particles-js">
        <div class="container-fluid carousel slide">
            <div class="row">
                <div class="col-lg-7 col-md-7 col-sm-12 col-xs-12">
                    <div class="slider-content">
                        <h2 data-animation="animated bounceInLeft" class="">VIFA PARTNER PROGRAM
                        </h2>
                        <h3 data-animation="animated bounceInLeft" class="">Give your customers the advantages of
                            accepting blockchain payments and growing their online business with DizPay while earning
                            extra revenue for your business.</h3>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <section class="section team-area cryp-otcarea bg-img">
        <div class="container">
            <div class="row">
                <div class="col-lg-12 col-md-12 col-sm-12 col-xs-12">
                    <div class="section-heading2 wow fadeInUp  animated" data-wow-duration="1.0s"
                        style="visibility: visible; animation-duration: 1s; animation-name: fadeInUp;">
                        <h3>Join the DizPay Global Partner Program</h3>
                    </div>
                </div>
            </div>
            <div class="row mt20">
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="dizpay_itme wow fadeInUp   animated" data-wow-duration="1.6s"
                        style="visibility: visible; animation-duration: 1.6s; animation-name: fadeInUp;">
                        <div class="box-with-icon text-center">
                            <div class="bwi-inner">
                                <div class="bwi-icon">
                                    <i class="fa fa-file-text-o"></i>
                                </div>
                                <h3 class="bwi-title">Submit an application</h3>
                                <p class="bwi-text">Tell us more about your business so we can match you to the
                                    right resources and benefits. There is no fee to join our Global Partner
                                    Program.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="dizpay_itme wow fadeInUp   animated" data-wow-duration="1.6s"
                        style="visibility: visible; animation-duration: 1.6s; animation-name: fadeInUp;">
                        <div class="box-with-icon text-center">
                            <div class="bwi-inner">
                                <div class="bwi-icon">
                                    <i class="fa fa-check-circle"></i>
                                </div>
                                <h3 class="bwi-title">Get certified</h3>
                                <p class="bwi-text">We'll provide you with resources and support to meet our
                                    certification requirements.</p>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="col-lg-4 col-md-4 col-sm-12 col-xs-12">
                    <div class="dizpay_itme wow fadeInUp   animated" data-wow-duration="1.6s"
                        style="visibility: visible; animation-duration: 1.6s; animation-name: fadeInUp;">
                        <div class="box-with-icon text-center">
                            <div class="bwi-inner">
                                <div class="bwi-icon">
                                    <i class="fa fa-external-link-square"></i>
                                </div>
                                <h3 class="bwi-title">Get started</h3>
                                <p class="bwi-text">Get access to exclusive marketing benefits designed to help you
                                    grow your merchant business.</p>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
